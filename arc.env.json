{
  "services": {
    "react-demo/frontend:latest": {
      "ingress": {
        "subdomain": "app"
      },
      "debug": {
        "path": "./src/frontend",
        "dockerfile": "Dockerfile.dev",
        "volumes": {
          "src": "./src/frontend/src"
        }
      }
    },
    "react-demo/backend:latest": {
      "ingress": {
        "subdomain": "api"
      },
      "debug": {
        "path": "./src/backend",
        "dockerfile": "Dockerfile.dev",
        "volumes": {
          "src": "./src/backend/src"
        }
      }
    }
  }
}
