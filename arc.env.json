{
  "services": {
    "react-demo/frontend:latest": {
      "ingress": {
        "subdomain": "app-main"
      },
      "debug": {
        "path": "./src/frontend",
        "dockerfile": "Dockerfile.dev"
      }
    },
    "react-demo/frontend-secondary:latest": {
      "ingress": {
        "subdomain": "app-secondary"
      },
      "debug": {
        "path": "./src/frontend-secondary",
        "dockerfile": "Dockerfile.dev"
      }
    },
    "react-demo/backend:latest": {
      "ingress": {
        "subdomain": "api"
      },
      "debug": {
        "path": "./src/backend",
        "dockerfile": "Dockerfile.dev"
      }
    }
  }
}
