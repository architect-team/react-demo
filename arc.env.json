{
  "services": {
    "react-demo/frontend:latest": {
      "ingress": {
        "subdomain": "app-main"
      },
      "debug": {
        "path": "./src/frontend",
        "dockerfile": "Dockerfile.dev"
      }
    },
    "react-demo/frontend-secondary:latest": {
      "ingress": {
        "subdomain": "app-secondary"
      },
      "debug": {
        "path": "./src/frontend-secondary",
        "dockerfile": "Dockerfile.dev"
      }
    },
    "react-demo/backend:latest": {
      "debug": {
        "path": "./src/backend",
        "dockerfile": "Dockerfile.dev"
      },
      "interfaces": {
        "main": {
          "description": "main port",
          "host": "main.host",
          "port": "8080"
        },
        "secondary": {
          "description": "secondary port",
          "host": "secondary.host",
          "port": "8081"
        }
      }
    }
  }
}
