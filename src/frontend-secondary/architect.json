{
  "name": "react-demo/frontend-secondary",
  "description": "test frontend service",
  "keywords": [
    ""
  ],
  "author": [
    "ryan-cahill"
  ],
  "license": "MIT",
  "dependencies": {
    "react-demo/backend": "latest"
  },
  "parameters": {
    "API_ADDR": {
      "default": {
        "valueFrom": {
          "dependency": "react-demo/backend:latest",
          "interface": "secondary",
          "value": "http://$HOST:$PORT"
        }
      }
    },
    "WORLD_TEXT": {
      "description": "greeting text on landing page",
      "default": "world"
    }
  },
  "datastores": {},
  "notifications": [],
  "subscriptions": {},
  "language": "node",
  "interfaces": {
    "main": 8080
  },
  "api": {
    "type": "rest"
  },
  "debug": {
    "command": "npm run dev",
    "volumes": {
      "src": {
        "mount_path": "/usr/src/app/src"
      }
    }
  }
}
