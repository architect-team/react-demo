{
  "name": "ryan-public-services/frontend",
  "description": "test frontend service",
  "keywords": [
    ""
  ],
  "author": "ryan-cahill",
  "dependencies": {
    "ryan-public-services/backend": "latest"
  },
  "parameters": {
    "API_ADDR": {
      "default": {
        "valueFrom": {
          "dependency": "ryan-public-services/backend:latest",
          "interface": "main",
          "value": "http://$HOST:$PORT"
        }
      }
    },
    "WORLD_TEXT": {
      "description": "greeting text on landing page",
      "default": "world"
    }
  },
  "datastores": {},
  "notifications": [],
  "subscriptions": {},
  "language": "node",
  "interfaces": {
    "main": 8080
  },
  "api": {
    "type": "rest"
  },
  "debug": {
    "command": "npm run dev",
    "volumes": {
      "src": {
        "mount_path": "/usr/src/app/src",
        "host_path": "./src"
      }
    }
  }
}
