{
  "name": "react-demo/frontend",
  "description": "test frontend service",
  "keywords": [
    ""
  ],
  "author": [
    "ryan-cahill"
  ],
  "license": "MIT",
  "dependencies": {
    "react-demo/backend": "latest"
  },
  "parameters": {
    "API_ADDR": {
      "default": {
        "valueFrom": {
          "dependency": "react-demo/backend:latest",
          "interface": "main",
          "value": "http://$EXTERNAL_HOST:$EXTERNAL_PORT"
        }
      }
    },
    "WORLD_TEXT": {
      "description": "greeting text on landing page",
      "default": "world"
    },
    "PORT": {
      "default": {
        "valueFrom": {
          "value": "$INTERNAL_PORT",
          "interface": "main",
          "self": true
        }
      }
    }
  },
  "datastores": {},
  "notifications": [],
  "subscriptions": {},
  "language": "node",
  "interfaces": {
    "main": {
      "port": "8080"
    }
  },
  "api": {
    "type": "rest"
  }
}
